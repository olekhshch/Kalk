{
  "productName": "calc",
  "version": "0.1.0",
  "identifier": "com.calc.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "calc",
        "width": 800,
        "height": 600,
        "decorations": false
        
      },
      {
        "label": "external",
        "title": "Tauri Docs",
        "url": "https://tauri.app"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
